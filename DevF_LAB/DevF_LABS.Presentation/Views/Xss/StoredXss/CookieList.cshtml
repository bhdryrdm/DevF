@model DevF_LABS.RequestResponse.XSS.StoredXSS.SXSS_S2_CookieListResponse
@{
    ViewBag.Title = "CookieList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<main class="main">
    <div class="card">

        <div class="card-body table-responsive">
            <table class="table table-hover table-bordered">
                <tr>
                    <th>ID</th>
                    <th>Cookie Name</th>
                    <th>Cookie Value</th>
                </tr>
                @if (Model.CookieList != null)
                {

                    foreach (var cookie in Model.CookieList)
                    {
                        <tr>
                            <td>@cookie.ID</td>
                            <td>@cookie.CookieName</td>
                            <td>@cookie.CookieValue</td>
                        </tr>
                    }
                }
            </table>
        </div>
    </div>
</main>
