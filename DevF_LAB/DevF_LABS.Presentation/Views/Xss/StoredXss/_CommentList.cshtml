@model DevF_LABS.RequestResponse.XSS.StoredXSS.SXSS_S1_CommentListResponse

@if (Model.CommentList != null)
{

    foreach (var comment in Model.CommentList)
    {
        <tr>
            <td>@comment.ID</td>
            <td>@comment.SXSS_S1_Name</td>
            <td>@comment.SXSS_S1_Email</td>
            <td>@comment.SXSS_S1_Comment</td>
            <td>@comment.CreatedTime</td>
            <td class="text-center">
                <button type="button" class="btn btn-info" data-toggle="modal" onclick="showComment(this)"><i class="fa fa-eye"></i></button>
            </td>
        </tr>
    }
}